<%= flash[:notice] %>
<h1>Admin login</h1><hr>
<p>Find me in app/views/admin/login.html.erb</p>
<%= form_with url: "/admin/login", method: :post do |form| %>
<div class= "field">
    <%= form.label :username %>
    <%= form.text_field :username%>
</div>
<div class= "field">
    <%= form.label :password %>
    <%= form.password_field :password%>
</div>
<div class= "actions">
    <%= form.submit "Admin Login" %>
</div>
<% end %>